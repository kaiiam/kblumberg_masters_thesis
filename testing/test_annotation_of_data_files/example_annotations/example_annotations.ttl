@prefix path: <PATH TO file.csv>.
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

######################################### csv files #########################################

#annotate a csv file. 
#file is about: 'X' in ('X')
path:file.csv a owl:NamedIndividual ;
			      obo:BFO_0000051 path:file.csvColumn1 ;
            obo:BFO_0000051 path:file.csvColumn2 ;
            obo:BFO_0000051 path:file.csvColumn3 ;
            #obo:BFO_0000051 path:file.csvColumnX ;
            # ...
            a [  rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( <http://purl.obolibrary.org/obo/OBCS_0000120>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.obolibrary.org/obo/IAO_0000136> ;
                                                              owl:someValuesFrom <'X'>
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                      ] 
              ] .

#annotate a csv file. 
#file is about: All Elements in ('X' or 'Y' or 'Z' or ...)
path:file2.csv a owl:NamedIndividual ;
            obo:BFO_0000051 path:file2.csvColumn1 ;
            obo:BFO_0000051 path:file2.csvColumn1 ;
            obo:BFO_0000051 path:file2.csvColumn3 ;
            #obo:BFO_0000051 path:file2.csvColumnX ;
            #...
            a [  rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( <http://purl.obolibrary.org/obo/OBCS_0000120>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.obolibrary.org/obo/IAO_0000136> ;
                                                              owl:someValuesFrom  [ rdf:type owl:Class ;
                                                                                    owl:unionOf ( <'X'>
                                                                                                  <'Y'>
                                                                                                )
                                                                                  ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                      ] 
              ] .

######################################### Columns #########################################

#is about some: ('X')
path:file.csvColumn1 a owl:NamedIndividual ;
				       a [rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                               owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000050> ;
                                                               owl:someValuesFrom <http://purl.obolibrary.org/obo/OBCS_0000120>
                                                              ]
                                                                                          
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://purl.obolibrary.org/obo/IAO_0000136> ;
                                                                owl:someValuesFrom < 'X'>
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                      ]
                 ] .

#is about some: ('X' and ('any property' some 'Y ') )
path:file.csvColumn2  a owl:NamedIndividual ;
                                        a [ rdf:type owl:Class ;
                                            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000050> ;
                                                                                         owl:someValuesFrom <http://purl.obolibrary.org/obo/OBCS_0000120>
                                                                                       ]                                           

                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://purl.obolibrary.org/obo/IAO_0000136> ;
                                                                                         owl:someValuesFrom [ owl:intersectionOf ( <'X '>
                                                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                                                       owl:onProperty <'any property'> ;
                                                                                                                                       owl:someValuesFrom <'Y'>
                                                                                                                                     ]
                                                                                                                                  ) ;
                                                                                                              rdf:type owl:Class
                                                                                                            ]
                                                                                       ]
                                                                                      ) ;
                                                                   rdf:type owl:Class
                                                                ]
                                          ].

#is about some: ('X' and ('any property1' some ('Y ' and ('any property2' some 'Z'))))
path:file.csvColumn3 a owl:NamedIndividual ;
                                        a [ rdf:type owl:Class ;
                                            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000050> ;
                                                                                         owl:someValuesFrom <http://purl.obolibrary.org/obo/OBCS_0000120>
                                                                                       ]

                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://purl.obolibrary.org/obo/IAO_0000136> ;
                                                                                         owl:someValuesFrom  [ owl:intersectionOf ( <'X'>
                                                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                                                       owl:onProperty <'any property1'> ;
                                                                                                                                       owl:someValuesFrom [ owl:intersectionOf ( <'Y'>
                                                                                                                                                                                    [ rdf:type owl:Restriction ;
                                                                                                                                                                                      owl:onProperty <'any property2'> ;
                                                                                                                                                                                      owl:someValuesFrom <'Z'>
                                                                                                                                                                                    ]
                                                                                                                                                                                  ) ;
                                                                                                                                                               rdf:type owl:Class
                                                                                                                                                            ]
                                                                                                                                      ]
                                                                                                                                  ) ;
                                                                                                                    rdf:type owl:Class
                                                                                                                ]
                                                                                          ]
                                                                                        ) ;
                                                                     rdf:type owl:Class
                                                                   ]
                                          ] .
