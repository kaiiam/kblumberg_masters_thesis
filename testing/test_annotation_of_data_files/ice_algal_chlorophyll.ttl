            obo:BFO_0000051 path:file2.csvColumn1 ;
            obo:BFO_0000051 path:file2.csvColumn3 ;
            #obo:BFO_0000051 path:file2.csvColumnX ;
            #...
            a [  rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( <http://purl.obolibrary.org/obo/OBCS_0000120>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.obolibrary.org/obo/IAO_0000136> ;
                                                              owl:someValuesFrom  [ owl:intersectionOf ( <http://purl.obolibrary.org/obo/PATO_0001018>
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty <http://purl.obolibrary.org/obo/RO_0000052> ;
                                                                                                           owl:someValuesFrom [ owl:intersectionOf ( <http://purl.obolibrary.org/obo/ENVO_00001999>
                                                                                                                                                    [ rdf:type owl:Restriction ;
                                                                                                                                                      owl:onProperty <http://purl.obolibrary.org/obo/RO_0002220> ;
                                                                                                                                                      owl:someValuesFrom <http://purl.obolibrary.org/obo/ENVO_00002200>
                                                                                                                                                    ]
                                                                                                                                                   ) ;
                                                                                                                                rdf:type owl:Class
                                                                                                                              ]
                                                                                                         ]
                                                                                                        ) ;
                                                                                     rdf:type owl:Class
                                                                                  ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                      ] 
              ] .

